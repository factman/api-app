{
  "name": "marketplace-api",
  "version": "1.0.0",
  "description": "Bezop Marketplace API",
  "main": "dist",
  "scripts": {
    "start": "NODE_ENV=development nodemon src --exec babel-node",
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel src -s -D -d dist --presets es2015,stage-2",
    "serve": "npm run -s build && NODE_ENV=production node ./dist",
    "test": "NODE_ENV=development  mocha --timeout 10000 --require babel-core/register",
    "kill": "lsof -n -iTCP:4000  -sTCP:LISTEN -n -l -P | grep 'LISTEN' | awk '{print $2}' | xargs kill -9",
    "doc": "rm -rf apidoc && apidoc -i src/ -o apidoc/ && opn apidoc/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bezopteam/marketplace-backend.git"
  },
  "keywords": [
    "Express",
    "RestAPI",
    "MongoDB",
    "Mongoose",
    "Marketplace"
  ],
  "author": "Winning Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bezopteam/marketplace-backend/issues"
  },
  "homepage": "https://github.com/bezopteam/marketplace-backend#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bodymen": "^1.1.0",
    "chai-http": "^4.0.0",
    "concat-stream": "^1.6.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "email-templates": "^4.0.1",
    "compression": "^1.7.3",
    "ethereumjs-util": "^5.1.5",
    "express": "^4.16.3",
    "express-force-ssl": "^0.3.2",
    "express-jwt": "^5.1.0",
    "jimp": "^0.2.28",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "mongoose": "^5.1.7",
    "multer": "^1.3.1",
    "pug": "^2.0.3",
    "streamifier": "^0.1.1",
    "morgan": "^1.9.0",
    "querymen": "^2.1.3",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "apidoc-markdown2": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coffeescript": "^2.3.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "opn-cli": "^3.1.0",
    "should": "^13.2.1",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": "8.11.3"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
